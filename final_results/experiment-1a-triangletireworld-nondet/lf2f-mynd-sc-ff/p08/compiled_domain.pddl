(define (domain triangle-tire)
	(:requirements :typing :strips :non-deterministic)
	(:constants )
	(:types location qstate)
	(:predicates (vehicleat ?loc - location) (sparein ?loc - location) (road ?from - location ?to - location) (notflattire) (prev_autstate ?q - qstate) (is_accepting ?q - qstate) (dummy_goal))
	(:action move-car
		:parameters (?from - location ?to - location)
		:precondition (and (vehicleat ?from) (road ?from ?to) (notflattire) )
		:effect (and 
			(when (prev_autstate q0)
			 (and (not (prev_autstate q0)) (prev_autstate q1))
			) 
			(when (and (prev_autstate q1) (not (vehicleat l1x1)))
			 (and )
			) 
			(when (and (prev_autstate q1) (vehicleat l1x1))
			 (and (not (prev_autstate q1)) (prev_autstate q3))
			) 
			(when (prev_autstate q2)
			 (and )
			) 
			(when (and (prev_autstate q3) (not (vehicleat l2x1)))
			 (and )
			) 
			(when (and (prev_autstate q3) (vehicleat l2x1))
			 (and (not (prev_autstate q3)) (prev_autstate q4))
			) 
			(when (and (prev_autstate q4) (not (vehicleat l1x2)))
			 (and )
			) 
			(when (and (prev_autstate q4) (vehicleat l1x2))
			 (and (not (prev_autstate q4)) (prev_autstate q5))
			) 
			(when (prev_autstate q5)
			 (and )
			) 
			(when (prev_autstate q6)
			 (and (not (prev_autstate q6)) (prev_autstate q2))
			) 
			(when (prev_autstate q0)
			 (and (not (prev_autstate q0)) (prev_autstate q1))
			) 
			(when (and (prev_autstate q1) (not (vehicleat l1x1)))
			 (and )
			) 
			(when (and (prev_autstate q1) (vehicleat l1x1))
			 (and (not (prev_autstate q1)) (prev_autstate q3))
			) 
			(when (prev_autstate q2)
			 (and )
			) 
			(when (and (prev_autstate q3) (not (vehicleat l2x1)))
			 (and )
			) 
			(when (and (prev_autstate q3) (vehicleat l2x1))
			 (and (not (prev_autstate q3)) (prev_autstate q4))
			) 
			(when (and (prev_autstate q4) (not (vehicleat l1x2)))
			 (and )
			) 
			(when (and (prev_autstate q4) (vehicleat l1x2))
			 (and (not (prev_autstate q4)) (prev_autstate q5))
			) 
			(when (prev_autstate q5)
			 (and )
			) 
			(when (prev_autstate q6)
			 (and (not (prev_autstate q6)) (prev_autstate q2))
			) 
			(oneof
			 (and (vehicleat ?to) (not (vehicleat ?from)))
			 (and (vehicleat ?to) (not (vehicleat ?from)) (not (notflattire)))
			))
	)
	(:action changetire
		:parameters (?loc - location)
		:precondition (and (sparein ?loc) (vehicleat ?loc) )
		:effect (and 
			(when (prev_autstate q0)
			 (and (not (prev_autstate q0)) (prev_autstate q1))
			) 
			(when (and (prev_autstate q1) (not (vehicleat l1x1)))
			 (and )
			) 
			(when (and (prev_autstate q1) (vehicleat l1x1))
			 (and (not (prev_autstate q1)) (prev_autstate q3))
			) 
			(when (prev_autstate q2)
			 (and )
			) 
			(when (and (prev_autstate q3) (not (vehicleat l2x1)))
			 (and )
			) 
			(when (and (prev_autstate q3) (vehicleat l2x1))
			 (and (not (prev_autstate q3)) (prev_autstate q4))
			) 
			(when (and (prev_autstate q4) (not (vehicleat l1x2)))
			 (and )
			) 
			(when (and (prev_autstate q4) (vehicleat l1x2))
			 (and (not (prev_autstate q4)) (prev_autstate q5))
			) 
			(when (prev_autstate q5)
			 (and )
			) 
			(when (prev_autstate q6)
			 (and (not (prev_autstate q6)) (prev_autstate q2))
			) 
			(when (prev_autstate q0)
			 (and (not (prev_autstate q0)) (prev_autstate q1))
			) 
			(when (and (prev_autstate q1) (not (vehicleat l1x1)))
			 (and )
			) 
			(when (and (prev_autstate q1) (vehicleat l1x1))
			 (and (not (prev_autstate q1)) (prev_autstate q3))
			) 
			(when (prev_autstate q2)
			 (and )
			) 
			(when (and (prev_autstate q3) (not (vehicleat l2x1)))
			 (and )
			) 
			(when (and (prev_autstate q3) (vehicleat l2x1))
			 (and (not (prev_autstate q3)) (prev_autstate q4))
			) 
			(when (and (prev_autstate q4) (not (vehicleat l1x2)))
			 (and )
			) 
			(when (and (prev_autstate q4) (vehicleat l1x2))
			 (and (not (prev_autstate q4)) (prev_autstate q5))
			) 
			(when (prev_autstate q5)
			 (and )
			) 
			(when (prev_autstate q6)
			 (and (not (prev_autstate q6)) (prev_autstate q2))
			) (notflattire) (not (sparein ?loc)))
	)
	(:action end
		:parameters (?q - qstate)
		:precondition (and (prev_autstate ?q) (is_accepting ?q) )
		:effect (dummy_goal)
	)
)
