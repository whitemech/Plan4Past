(define (domain blocks-domain)
    (:requirements :conditional-effects :derived-predicates :negative-preconditions :strips)
    (:types block)
    (:predicates (clear ?x) (emptyhand) (holding ?x) (on ?x ?y) (ontable ?x) (val_on_b_c) (val_on_c_a) (val_on_d_i) (val_on_e_b) (val_on_f_g) (val_on_f_g-and-on_g_h-and-on_h_d-and-on_d_i-and-on_i_e-and-on_e_b-and-on_b_c-and-on_c_a) (val_on_g_h) (val_on_h_d) (val_on_i_e))
    (:derived (val_on_b_c) (on b c))
     (:derived (val_on_c_a) (on c a))
     (:derived (val_on_d_i) (on d i))
     (:derived (val_on_e_b) (on e b))
     (:derived (val_on_f_g) (on f g))
     (:derived (val_on_f_g-and-on_g_h-and-on_h_d-and-on_d_i-and-on_i_e-and-on_e_b-and-on_b_c-and-on_c_a) (and (val_on_f_g) (val_on_g_h) (val_on_h_d) (val_on_d_i) (val_on_i_e) (val_on_e_b) (val_on_b_c) (val_on_c_a)))
     (:derived (val_on_g_h) (on g h))
     (:derived (val_on_h_d) (on h d))
     (:derived (val_on_i_e) (on i e))
    (:action pick-up
        :parameters (?x)
        :precondition (and (clear ?x) (ontable ?x) (emptyhand))
        :effect (and (and (not (ontable ?x)) (not (clear ?x)) (not (emptyhand)) (holding ?x)))
    )
     (:action put-down
        :parameters (?x)
        :precondition (holding ?x)
        :effect (and (and (not (holding ?x)) (clear ?x) (emptyhand) (ontable ?x)))
    )
     (:action stack
        :parameters (?x ?y)
        :precondition (and (holding ?x) (clear ?y))
        :effect (and (and (not (holding ?x)) (not (clear ?y)) (clear ?x) (emptyhand) (on ?x ?y)))
    )
     (:action unstack
        :parameters (?x ?y)
        :precondition (and (on ?x ?y) (clear ?x) (emptyhand))
        :effect (and (and (holding ?x) (clear ?y) (not (clear ?x)) (not (emptyhand)) (not (on ?x ?y))))
    )
)