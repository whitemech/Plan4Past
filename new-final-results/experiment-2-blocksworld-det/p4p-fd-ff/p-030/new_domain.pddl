(define (domain blocks-domain)
    (:requirements :conditional-effects :derived-predicates :negative-preconditions :strips)
    (:types block)
    (:predicates (clear ?x) (emptyhand) (holding ?x) (on ?x ?y) (ontable ?x) (val_on_a_i) (val_on_b_h) (val_on_d_b) (val_on_e_l) (val_on_f_g) (val_on_g_a) (val_on_h_m) (val_on_i_e) (val_on_j_d) (val_on_j_d-and-on_d_b-and-on_b_h-and-on_h_m-and-on_m_k-and-on_k_f-and-on_f_g-and-on_g_a-and-on_a_i-and-on_i_e-and-on_e_l-and-on_l_n-and-on_n_c) (val_on_k_f) (val_on_l_n) (val_on_m_k) (val_on_n_c))
    (:derived (val_on_a_i) (on a i))
     (:derived (val_on_b_h) (on b h))
     (:derived (val_on_d_b) (on d b))
     (:derived (val_on_e_l) (on e l))
     (:derived (val_on_f_g) (on f g))
     (:derived (val_on_g_a) (on g a))
     (:derived (val_on_h_m) (on h m))
     (:derived (val_on_i_e) (on i e))
     (:derived (val_on_j_d) (on j d))
     (:derived (val_on_j_d-and-on_d_b-and-on_b_h-and-on_h_m-and-on_m_k-and-on_k_f-and-on_f_g-and-on_g_a-and-on_a_i-and-on_i_e-and-on_e_l-and-on_l_n-and-on_n_c) (and (val_on_j_d) (val_on_d_b) (val_on_b_h) (val_on_h_m) (val_on_m_k) (val_on_k_f) (val_on_f_g) (val_on_g_a) (val_on_a_i) (val_on_i_e) (val_on_e_l) (val_on_l_n) (val_on_n_c)))
     (:derived (val_on_k_f) (on k f))
     (:derived (val_on_l_n) (on l n))
     (:derived (val_on_m_k) (on m k))
     (:derived (val_on_n_c) (on n c))
    (:action pick-up
        :parameters (?x)
        :precondition (and (clear ?x) (ontable ?x) (emptyhand))
        :effect (and (and (not (ontable ?x)) (not (clear ?x)) (not (emptyhand)) (holding ?x)))
    )
     (:action put-down
        :parameters (?x)
        :precondition (holding ?x)
        :effect (and (and (not (holding ?x)) (clear ?x) (emptyhand) (ontable ?x)))
    )
     (:action stack
        :parameters (?x ?y)
        :precondition (and (holding ?x) (clear ?y))
        :effect (and (and (not (holding ?x)) (not (clear ?y)) (clear ?x) (emptyhand) (on ?x ?y)))
    )
     (:action unstack
        :parameters (?x ?y)
        :precondition (and (on ?x ?y) (clear ?x) (emptyhand))
        :effect (and (and (holding ?x) (clear ?y) (not (clear ?x)) (not (emptyhand)) (not (on ?x ?y))))
    )
)