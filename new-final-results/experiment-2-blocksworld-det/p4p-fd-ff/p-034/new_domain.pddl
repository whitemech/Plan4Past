(define (domain blocks-domain)
    (:requirements :conditional-effects :derived-predicates :negative-preconditions :strips)
    (:types block)
    (:predicates (clear ?x) (emptyhand) (holding ?x) (on ?x ?y) (ontable ?x) (val_on_a_p) (val_on_b_k) (val_on_c_l) (val_on_d_h) (val_on_e_c) (val_on_f_b) (val_on_g_e) (val_on_h_f) (val_on_i_d) (val_on_i_d-and-on_d_h-and-on_h_f-and-on_f_b-and-on_b_k-and-on_k_j-and-on_j_g-and-on_g_e-and-on_e_c-and-on_c_l-and-on_l_m-and-on_m_n-and-on_n_a-and-on_a_p-and-on_p_o) (val_on_j_g) (val_on_k_j) (val_on_l_m) (val_on_m_n) (val_on_n_a) (val_on_p_o))
    (:derived (val_on_a_p) (on a p))
     (:derived (val_on_b_k) (on b k))
     (:derived (val_on_c_l) (on c l))
     (:derived (val_on_d_h) (on d h))
     (:derived (val_on_e_c) (on e c))
     (:derived (val_on_f_b) (on f b))
     (:derived (val_on_g_e) (on g e))
     (:derived (val_on_h_f) (on h f))
     (:derived (val_on_i_d) (on i d))
     (:derived (val_on_i_d-and-on_d_h-and-on_h_f-and-on_f_b-and-on_b_k-and-on_k_j-and-on_j_g-and-on_g_e-and-on_e_c-and-on_c_l-and-on_l_m-and-on_m_n-and-on_n_a-and-on_a_p-and-on_p_o) (and (val_on_i_d) (val_on_d_h) (val_on_h_f) (val_on_f_b) (val_on_b_k) (val_on_k_j) (val_on_j_g) (val_on_g_e) (val_on_e_c) (val_on_c_l) (val_on_l_m) (val_on_m_n) (val_on_n_a) (val_on_a_p) (val_on_p_o)))
     (:derived (val_on_j_g) (on j g))
     (:derived (val_on_k_j) (on k j))
     (:derived (val_on_l_m) (on l m))
     (:derived (val_on_m_n) (on m n))
     (:derived (val_on_n_a) (on n a))
     (:derived (val_on_p_o) (on p o))
    (:action pick-up
        :parameters (?x)
        :precondition (and (clear ?x) (ontable ?x) (emptyhand))
        :effect (and (and (not (ontable ?x)) (not (clear ?x)) (not (emptyhand)) (holding ?x)))
    )
     (:action put-down
        :parameters (?x)
        :precondition (holding ?x)
        :effect (and (and (not (holding ?x)) (clear ?x) (emptyhand) (ontable ?x)))
    )
     (:action stack
        :parameters (?x ?y)
        :precondition (and (holding ?x) (clear ?y))
        :effect (and (and (not (holding ?x)) (not (clear ?y)) (clear ?x) (emptyhand) (on ?x ?y)))
    )
     (:action unstack
        :parameters (?x ?y)
        :precondition (and (on ?x ?y) (clear ?x) (emptyhand))
        :effect (and (and (holding ?x) (clear ?y) (not (clear ?x)) (not (emptyhand)) (not (on ?x ?y))))
    )
)