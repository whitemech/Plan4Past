(define (domain blocks-domain)
    (:requirements :conditional-effects :derived-predicates :negative-preconditions :strips)
    (:types block)
    (:predicates (clear ?x) (emptyhand) (holding ?x) (on ?x ?y) (ontable ?x) (val_on_a_h) (val_on_b_j) (val_on_c_k) (val_on_d_m) (val_on_e_l) (val_on_e_l-and-on_l_f-and-on_f_b-and-on_b_j-and-on_j_i-and-on_i_n-and-on_n_c-and-on_c_k-and-on_k_g-and-on_g_d-and-on_d_m-and-on_m_a-and-on_a_h) (val_on_f_b) (val_on_g_d) (val_on_i_n) (val_on_j_i) (val_on_k_g) (val_on_l_f) (val_on_m_a) (val_on_n_c))
    (:derived (val_on_a_h) (on a h))
     (:derived (val_on_b_j) (on b j))
     (:derived (val_on_c_k) (on c k))
     (:derived (val_on_d_m) (on d m))
     (:derived (val_on_e_l) (on e l))
     (:derived (val_on_e_l-and-on_l_f-and-on_f_b-and-on_b_j-and-on_j_i-and-on_i_n-and-on_n_c-and-on_c_k-and-on_k_g-and-on_g_d-and-on_d_m-and-on_m_a-and-on_a_h) (and (val_on_e_l) (val_on_l_f) (val_on_f_b) (val_on_b_j) (val_on_j_i) (val_on_i_n) (val_on_n_c) (val_on_c_k) (val_on_k_g) (val_on_g_d) (val_on_d_m) (val_on_m_a) (val_on_a_h)))
     (:derived (val_on_f_b) (on f b))
     (:derived (val_on_g_d) (on g d))
     (:derived (val_on_i_n) (on i n))
     (:derived (val_on_j_i) (on j i))
     (:derived (val_on_k_g) (on k g))
     (:derived (val_on_l_f) (on l f))
     (:derived (val_on_m_a) (on m a))
     (:derived (val_on_n_c) (on n c))
    (:action pick-up
        :parameters (?x)
        :precondition (and (clear ?x) (ontable ?x) (emptyhand))
        :effect (and (and (not (ontable ?x)) (not (clear ?x)) (not (emptyhand)) (holding ?x)))
    )
     (:action put-down
        :parameters (?x)
        :precondition (holding ?x)
        :effect (and (and (not (holding ?x)) (clear ?x) (emptyhand) (ontable ?x)))
    )
     (:action stack
        :parameters (?x ?y)
        :precondition (and (holding ?x) (clear ?y))
        :effect (and (and (not (holding ?x)) (not (clear ?y)) (clear ?x) (emptyhand) (on ?x ?y)))
    )
     (:action unstack
        :parameters (?x ?y)
        :precondition (and (on ?x ?y) (clear ?x) (emptyhand))
        :effect (and (and (holding ?x) (clear ?y) (not (clear ?x)) (not (emptyhand)) (not (on ?x ?y))))
    )
)